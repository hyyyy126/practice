#include <stdio.h>
int main()
{
	int a = 1;
	int b = 1;
	for (a = 1;  a <= 9; a++)
	{
		for (b = 1; b <= 9; b++)
		{
			int x = a * b;
			printf("%d * %d = %d ", a, b, x);
				if (x % 9 == 0 && a!=3 && a!=6 && a!=9)
				{
					printf("\n");
				}
				if (a==3 && b==9)
				{
					printf("\n");
				}		
				if (a == 6 && b == 9)
				{
					printf("\n");
				}
		}
	}
	return 0;
}
