int main()
{
	long int m = 3;
	long long int n = 6;
	unsigned char a = 1;
	double h = 2.5;
	
	printf ("%zu\n", (sizeof m));
	printf ("%zu\n", (sizeof n));
	printf ("%zu\n", (sizeof a));
	printf ("%zu\n", (sizeof h));
	printf("\a");

	return 0;

}
